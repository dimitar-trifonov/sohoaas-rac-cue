# Google OAuth2 Credentials
# Get these from Google Cloud Console > APIs & Services > Credentials
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# OAuth2 Redirect URL (adjust for your deployment)
OAUTH_REDIRECT_URL=http://localhost:8080/api/auth/callback

# Server Configuration
PORT=8080

# Frontend Configuration
REACT_APP_SERVICE_PROXY_URL=http://localhost:8080
REACT_APP_MCP_WEBSOCKET_URL=ws://localhost:8080/mcp

# Genkit Configuration
GENKIT_PORT=4000
GOOGLE_AI_API_KEY=your_google_ai_api_key_here
MCP_SERVER_URL=ws://localhost:8080/mcp

# Optional: Database configuration (if using postgres profile)
# POSTGRES_DB=service_proxies
# POSTGRES_USER=proxy_user
# POSTGRES_PASSWORD=proxy_password

# Optional: Redis configuration (if using redis profile)
# REDIS_URL=redis://localhost:6379
